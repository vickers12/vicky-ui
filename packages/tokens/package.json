{
    "name": "@vicky-ui/tokens",
    "version": "2.0.0",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "./tokens.css": "./dist/tokens.css",
        "./tokens.scss": "./dist/tokens.scss",
        "./fonts.css": "./dist/fonts.css",
        "./tokens.panda.json": "./dist/tokens.panda.json",
        "./types": {
            "import": "./dist/types/index.js",
            "types": "./dist/types/index.d.ts"
        }
    },
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "tsx src/style-dictionary/build.ts && pnpm run build:types",
        "build:types": "tsc --project tsconfig.types.json"
    },
    "devDependencies": {
        "style-dictionary": "^5.0.2"
    },
    "sideEffects": false
}
